    package="com.example.apeptodaygroep4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_mario"
        android:label="@string/appName"
        android:roundIcon="@drawable/mariohead"
        android:supportsRtl="true"
        android:theme="@style/Theme.APEPToDayGroep4">
        <activity android:name=".UserActivity.UpdateLabel" />
        <activity android:name=".UserActivity.EditLabel" />
        <activity android:name=".UserActivity.AddLabel" />
        <activity android:name=".ShowTaskActivity" />
        <activity android:name=".FinishedTasks" />
